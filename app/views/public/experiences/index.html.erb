<% content_for :title do %>
  Resume
<% end %>

<p id="notice"><%= notice %></p>

<h1>Resume</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @experiences.each do |experience| %>
      <tr>
        <strong><%= experience.organization %></strong>
        <p><%= experience.title %></p>
        <p><%= experience.started_at %> - <%= experience.ended_at %></p>
        <p><%= link_to 'Details', experience %></p>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
